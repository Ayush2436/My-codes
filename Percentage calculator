name = input("Write your Name ")
print ("Write Marsks of your All subject:-")
H = int(input ("Hindi - "))
E = int(input ("English - "))
P = int(input ("Physics - "))
C = int(input ("Chemistry - "))
M = int(input ("Maths - "))
marks = ((H+E+P+C+M)/5)
print ("Your Percentage is ",marks,'%')
if (marks >90):
   print ('Congratulations!',name)
   print ('''Brilliant! you got 90%+,
